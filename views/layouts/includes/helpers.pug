mixin breadcrumbs(links,titles)
  nav(aria-label="Breadcrumb",class="breadcrumb")
    ul.breadcrumb
      li
        a(href="/")
          | Home
      - var i = 0;
      - var linkCount = links.length
      - while ( i < linkCount - 1 )
        li
          a(href=links[i])
            | #{titles[i]}
        - i++;
      li.active(aria-current="page")
        | #{titles[linkCount - 1]}
